<?xml version="1.0"?>
<launch>
    <arg name="bag_file" default="$(find group3_lab5)/bags/free.bag" />
    <node pkg="rosbag"
          type="play"
          name="playback"
          output="screen"
          args="--loop --clock $(arg bag_file)"/>
    <node name="plotjuggler" 
          pkg="plotjuggler"
          type="PlotJuggler"
          args="--layout $(find group3_lab5)/launch/plotjuggler_layout.xml"/>
</launch>